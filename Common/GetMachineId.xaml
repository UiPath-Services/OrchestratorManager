  <Sequence sap2010:Annotation.AnnotationText="Get Machine ID based on its name.&#xA;Limitation: Machine whose names are composed only of digits are not supported." DisplayName="Get Machine ID" sap2010:WorkflowViewState.IdRef="Sequence_22">
    <TryCatch DisplayName="Try Catch" sap2010:WorkflowViewState.IdRef="TryCatch_1">
      <TryCatch.Try>
        <If Condition="[in_MachineNameOrID.All(Function(c) Char.IsDigit(c))]" DisplayName="If Machine Name Or ID Is Numeric" sap2010:WorkflowViewState.IdRef="If_12">
