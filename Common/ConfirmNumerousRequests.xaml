  <Sequence sap2010:Annotation.AnnotationText="Confirm whether the number of planned requests is greater than the specified threshold.&#xA;If it it, ask user confirmation." DisplayName="Confirm Numerous Requests" sap2010:WorkflowViewState.IdRef="Sequence_7">
    <If Condition="[in_PlannedNumberOfRequests &gt; in_Config(&quot;MaximumRequestsThreshold&quot;)]" DisplayName="If Number of Requests Is Larger than Threshold" sap2010:WorkflowViewState.IdRef="If_5">
      <If.Then>
        <Sequence DisplayName="Large Number of Requests" sap2010:WorkflowViewState.IdRef="Sequence_6">
          <Sequence.Variables>
            <Variable x:TypeArguments="x:String" Name="SelectedButton" />
          </Sequence.Variables>
          <ui:MessageBox Buttons="YesNo" Caption="Large Number of Operations" ChosenButton="[SelectedButton]" DisplayName="Message Box (Confirm Large Number of Operations)" sap2010:WorkflowViewState.IdRef="MessageBox_1" Text="[in_Config(&quot;ConfirmNumerousRequests&quot;)]" TopMost="True" />
          <If Condition="[SelectedButton = &quot;No&quot;]" DisplayName="If Process Should Be Interrupted" sap2010:WorkflowViewState.IdRef="If_4">
            <If.Then>
              <TerminateWorkflow DisplayName="Terminate Workflow" sap2010:WorkflowViewState.IdRef="TerminateWorkflow_1" Reason="User terminated the processed due to a large number of requests that would be performed." />
            </If.Then>
            <If.Else>
              <ui:LogMessage DisplayName="Log Message (Numerous Requests Confirmed)" sap2010:WorkflowViewState.IdRef="LogMessage_1" Level="Warn" Message="[in_Config(&quot;NumerousRequestsConfirmed&quot;)]" />
            </If.Else>
          </If>
        </Sequence>
      </If.Then>
    </If>
    <sads:DebugSymbol.Symbol></sads:DebugSymbol.Symbol>
  </Sequence>
